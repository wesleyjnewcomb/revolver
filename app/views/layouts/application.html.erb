<!DOCTYPE html>
<html>
  <head>
    <title>Revolver</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <nav>
      <%= link_to "Sign Up", new_user_registration_path %>
